.contact-container {
    padding: 40rpx 30rpx;
}

.contact-header {
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    line-height: 28rpx;
    font-weight: bold;
    padding-bottom: 25rpx;
}

.contact-item {
    padding: 15rpx 0;
    font-size: 28rpx;
}

.contact-phone-title {
    display: inline-block;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    font-weight: bold;
}

.contact-phone {
    display: inline-block;
    padding-right: 30rpx;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    font-weight: bold;
}

.contact-name-title {
    display: inline-block;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #6CAB3B;
    font-weight: bold;
}

.contact-name {
    display: inline-block;
    padding-right: 30rpx;
    color: #6CAB3B;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    font-weight: bold;
}

.contact-copy {
    display: inline-block;
    padding: 4rpx 16rpx;
    border-radius: 12rpx;
    background: #6CAB3B;
    font-family: PingFangSC-Medium;
    font-size: 26rpx;
    color: white;
}

.contact-back {
    position: absolute;
    bottom: 100rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #6CAB3B;
    padding: 16rpx 0;
    width: 300rpx;
    text-align: center;
    border-radius: 12rpx;
    border: 1rpx solid #6CAB3B;
    left: 50%;
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    transform: translateX(-50%);
}